{
  "name": "vue-action",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:es": "eslint src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.vue",
    "spellcheck": "cspell lint --dot --gitignore --color --cache \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "type-check": "vue-tsc -b --noEmit",
    "lint:stage": "lint-staged",
    "lint": "npm-run-all lint:es lint:style",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "eslint-plugin-simple-import-sort": "12.1.1",
    "pinia": "2.1.7",
    "vue": "3.4.31",
    "vue-router": "4.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.7.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "commitlint": "19.3.0",
    "cspell": "8.10.4",
    "cypress": "^12.14.0",
    "cz-git": "1.12.0",
    "eslint": "9.7.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "9.27.0",
    "husky": "9.0.11",
    "jsdom": "^22.1.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "stylelint": "16.7.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0",
    "vite": "5.3.3",
    "vitest": "^0.32.0",
    "vue-tsc": "2.0.26",
    "zx": "^8.8.5"
  }
}
